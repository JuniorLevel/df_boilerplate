[version]
0.250.0
[ignore]
<PROJECT_ROOT>/node_modules/
<PROJECT_ROOT>/github-pages/
<PROJECT_ROOT>/__snapshots__/
<PROJECT_ROOT>/dist/
[include]
src/**/*.{js,jsx} 
[libs]

[lints]
untyped-type-import=error
internal-type=error
deprecated-type-bool=error
[options]
module.file_ext=.jsx
module.file_ext=.js
module.file_ext=.styles.js
module.file_ext=.module.css
module.file_ext=.css
module.file_ext=.module.scss
module.file_ext=.scss
autoimports=true
react.runtime=automatic
[strict]
